{

    "manifest_version": 2,
    "name": "Wikipedia Backlinks Sidebar",
    "version": "1.0",
  
    "description": "Adds backlinks to Wikipedia pages",


    "sidebar_action": {
        "default_icon": "icons/baseline_link_black_18dp.png",
        "default_title" : "Wikipedia Backlinks",
        "default_panel": "panel/panel.html"
      },
      
    "permissions": ["storage", 
        "tabs",
        "*://*/*"],

    "commands": {
        "_execute_sidebar_action": {
          "suggested_key": {
            "default": "Ctrl+Shift+Y"
          }
        }
    }
  
  }